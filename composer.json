{
    "name": "esoftslimited/yii2-plugins-system",
    "description": "Plugins system as WP for Yii2 copied from loveorigami for learning purpose",
    "keywords": ["yii2", "plugin", "event", "yii2-plugin"],
    "type": "yii2-extension",
    "license": "MIT",
    "support": {
        "source": "https://github.com/loveorigami/yii2-plugins-system",
        "issues": "https://github.com/loveorigami/yii2-plugins-system/issues"
    },
    "authors": [
        {
            "name": "Plugin Systems",
            "homepage": "https://github.com/loveorigami/"
        }
    ],
    "require": {
        "yiisoft/yii2": ">=2.0.6",
        "components/highlightjs":"*",
        "loveorigami/yii2-jsoneditor": "*",
        "tpoxa/shortcodes": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "esoftslimited\\plugins\\": ""
        }
    },
    "extra": {
        "bootstrap": "esoftslimited\\plugins\\Bootstrap",        
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
